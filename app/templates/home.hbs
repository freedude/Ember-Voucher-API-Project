<header class="header">

</header>


{{#each this.model as |home|}}
  <article class="listing">
    <div class="details">
      <h3>{{home.title}}</h3>
      <div class="detail owner">
        <span>Owner:</span> {{home.owner}}
      </div>
      <div class="detail type">
        <span>Type:</span> {{home.category}}
      </div>
      <div class="deatail location">
        <span>Location:</span> {{home.city}}
      </div>
      <div class="detail bedrooms">
        <span>Number of bedrooms:</span> {{home.bedrooms}}
      </div>
    </div>
  </article>
{{/each}}