<div class="container-fluid">
    <div class="row">
        <div class="col-12 mt-3">
            <div class="card">
                <div class="card-horizontal">
                    <div class="img-square-wrapper">
                        <img class="" src="http://via.placeholder.com/300x180" alt="Card image cap">
                    </div>
                    <div class="card-body">
                       
                      
    {{#each this.card as |cardItem|}}
        <h5>
            {{component
                this.cardComponent
                item=cardItem
            }}
        </h5>
        {{/each}}
                    </div>
                </div>
                {{!-- <div class="card-footer">
                    <a href="#" class="btn btn-primary">Create Voucher</a>
                </div> --}}
            </div>
        </div>
    </div>
</div>



